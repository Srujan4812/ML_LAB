# Step 1 (i): Load CSV file into DataFrame
import pandas as pd

df = pd.read_csv("housing.csv")

# Step 1 (ii): Display information of all columns
print(df.info())

# Step 1 (iii): Display statistical information of all numerical columns
print(df.describe())

# Step 1 (iv): Display count of unique labels for "ocean_proximity" column
print(df["ocean_proximity"].value_counts())

# Step 1 (v): Display attributes having missing values count > 0
print(df.isnull().sum()[df.isnull().sum() > 0])
